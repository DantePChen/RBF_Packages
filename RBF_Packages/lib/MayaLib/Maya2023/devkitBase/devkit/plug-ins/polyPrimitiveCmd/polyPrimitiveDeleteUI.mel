global proc polyPrimitiveDeleteUI()
{
	global string $gMainPrimitivesMenu;

	if (`menu -exists $gMainPrimitivesMenu`)
	{
		if (`menuItem -exists polyPrimitivesItem`)
		{
			deleteUI -menuItem polyPrimitiveWindowItem;
		}
	}
}
